+	ERRCODE_ALREADY = 0x08,
+	ERRCODE_NOTIMPL = 0x09,
+	ERRCODE_RANGE = 0x0A,
+
+	/*   Reserved 0x0B - 0xFE */
+
+	ERRCODE_UNKNOWN = 0xFF,
+} RETCODE;
