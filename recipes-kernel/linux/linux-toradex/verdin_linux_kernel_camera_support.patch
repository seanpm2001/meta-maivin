+static int mcu_stream_config(struct i2c_client *client, uint32_t format,
+			     int mode, int frate_index)
+{
+	uint32_t payload_len = 0;
+
+	uint16_t cmd_status = 0, index = 0xFFFF;
+	uint8_t retcode = 0, cmd_id = 0;
+	int loop = 0, ret = 0, err = 0, retry = 1000;
+	static uint16_t prev_index = 0xFFFE;
+
+	/* lock semaphore */
+	mutex_lock(&mcu_i2c_mutex_0234);
+
+	cmd_id = CMD_ID_STREAM_CONFIG;
+	if (mcu_get_cmd_status(client, &cmd_id, &cmd_status, &retcode) < 0) {
+		dev_err(&client->dev," %s(%d) Error \n", __func__, __LINE__);
+		ret = -EIO;
+		goto exit;
+	}
+
+	if ((cmd_status != MCU_CMD_STATUS_SUCCESS) ||
+	    (retcode != ERRCODE_SUCCESS)) {
+		dev_err(&client->dev,
+			" ISP is Unintialized or Busy STATUS = 0x%04x Errcode = 0x%02x !! \n",
+		     cmd_status, retcode);
+		ret = -EBUSY;
+		goto exit;
+	}
+
+	for (loop = 0;(&streamdb[loop]) != NULL; loop++) {
+		pr_info("streamdb[%d]=%d, mode=%d",loop,streamdb[loop],mode);
+		if (streamdb[loop] == mode) {
+			index = loop + frate_index;
+			break;
+		}
+	}
+
+#ifdef AR0234_DEBUG
+	pr_info(" Index = 0x%04x , format = 0x%08x, width = %hu,"
+		     " height = %hu, frate num = %hu \n", index, format,
+		     ar0234_data.mcu_cam_frmfmt[mode].size.width,
+		     ar0234_data.mcu_cam_frmfmt[mode].size.height,
+		     ar0234_data.mcu_cam_frmfmt[mode].framerates[frate_index]);
+#endif
+
+	if (index == 0xFFFF) {
+		ret = -EINVAL;
+		goto exit;
+	}
+
+	if(prev_index == index) {
+#ifdef AR0234_DEBUG
+		pr_info("Skipping Previous mode set ... \n");
+#endif
+		ret = 0;
+		goto exit;
+	}
+
+issue_cmd:
+	/* First Txn Payload length = 0 */
+	payload_len = 14;
+
+	mc_data_0234[0] = CMD_SIGNATURE;
+	mc_data_0234[1] = CMD_ID_STREAM_CONFIG;
+	mc_data_0234[2] = payload_len >> 8;
+	mc_data_0234[3] = payload_len & 0xFF;
+	mc_data_0234[4] = errorcheck(&mc_data_0234[2], 2);
+
+	ar0234_write(client, mc_data_0234, TX_LEN_PKT);
+
+	mc_data_0234[0] = CMD_SIGNATURE;
+	mc_data_0234[1] = CMD_ID_STREAM_CONFIG;
+	mc_data_0234[2] = index >> 8;
+	mc_data_0234[3] = index & 0xFF;
+
+	/* Format Fourcc - currently only YUYV */
+	mc_data_0234[4] = format >> 24;
+	mc_data_0234[5] = format >> 16;
+	mc_data_0234[6] = format >> 8;
+	mc_data_0234[7] = format & 0xFF;
+
+	/* width */
+	mc_data_0234[8] = ar0234_data.mcu_cam_frmfmt[mode].size.width >> 8;
+	mc_data_0234[9] = ar0234_data.mcu_cam_frmfmt[mode].size.width & 0xFF;
+
+	/* height */
+	mc_data_0234[10] = ar0234_data.mcu_cam_frmfmt[mode].size.height >> 8;
+	mc_data_0234[11] = ar0234_data.mcu_cam_frmfmt[mode].size.height & 0xFF;
+
+	/* frame rate num */
+	mc_data_0234[12] = ar0234_data.mcu_cam_frmfmt[mode].framerates[frate_index] >> 8;
+	mc_data_0234[13] = ar0234_data.mcu_cam_frmfmt[mode].framerates[frate_index] & 0xFF;
+
+	/* frame rate denom */
+	mc_data_0234[14] = 0x00;
+	mc_data_0234[15] = 0x01;
+
+	mc_data_0234[16] = errorcheck(&mc_data_0234[2], 14);
+	err = ar0234_write(client, mc_data_0234, 17);
+	if (err != 0) {
+		dev_err(&client->dev," %s(%d) Error - %d \n", __func__,
+		       __LINE__, err);
+		ret = -EIO;
+		goto exit;
+	}
+
+	while (--retry > 0) {
+		cmd_id = CMD_ID_STREAM_CONFIG;
+		if (mcu_get_cmd_status(
+			client, &cmd_id, &cmd_status, &retcode) < 0) {
+			dev_err(&client->dev,
+				" %s(%d) MCU GET CMD Status Error : loop : %d \n",
+				__func__, __LINE__, loop);
+			ret = -EIO;
+			goto exit;
+		}
+
+		if ((cmd_status == MCU_CMD_STATUS_SUCCESS) &&
+		    (retcode == ERRCODE_SUCCESS)) {
+			ret = 0;
+			goto exit;
+		}
+
+		if(retcode == ERRCODE_AGAIN) {
+			/* Issue Command Again if Set */
+			retry = 1000;
+			goto issue_cmd;
+		}
+
+		if ((retcode != ERRCODE_BUSY) &&
+		    ((cmd_status != MCU_CMD_STATUS_PENDING))) {
+			dev_err(&client->dev,
+				"(%s) %d Error STATUS = 0x%04x RET = 0x%02x\n",
+				__func__, __LINE__, cmd_status, retcode);
+			ret = -EIO;
+			goto exit;
+		}
+
+		/* Delay after retry */
+		mdelay(10);
+	}
+
+	dev_err(&client->dev," %s(%d) Error - %d \n", __func__,
+			__LINE__, err);
+	ret = -ETIMEDOUT;
+
+exit:
+	if(!ret)
+		prev_index = index;
+
+	/* unlock semaphore */
+	mutex_unlock(&mcu_i2c_mutex_0234);
+
+	return ret;
+}
