+typedef enum _errno {
+	ERRCODE_SUCCESS = 0x00,
+	ERRCODE_BUSY = 0x01,
+	ERRCODE_INVAL = 0x02,
+	ERRCODE_PERM = 0x03,
+	ERRCODE_NODEV = 0x04,
+	ERRCODE_IO = 0x05,
+	ERRCODE_HW_SPEC = 0x06,
+	ERRCODE_AGAIN = 0x07,
