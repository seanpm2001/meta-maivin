+/*
+ * mcu_get_fw_version:
+ *
+ * Read the firmware version from the MCU.
+ *
+ * A success value (0) is returned when the MCU version could be successfully read.
+ * else a negative value indicating error is returned.
+ */
+static int mcu_get_fw_version(struct i2c_client *client, unsigned char *fw_version_0234)
+{
+	uint32_t payload_len = 0;
+	uint8_t errcode = ERRCODE_SUCCESS, orig_crc = 0, calc_crc = 0;
+	int ret = 0, err = 0, loop;
+	/* Query firmware version from MCU */
+
+	/* lock semaphore */
+	mutex_lock(&mcu_i2c_mutex_0234);
+
+	mc_data_0234[0] = CMD_SIGNATURE;
+	mc_data_0234[1] = CMD_ID_VERSION;
+	mc_data_0234[2] = payload_len >> 8;
+	mc_data_0234[3] = payload_len & 0xFF;
+	mc_data_0234[4] = errorcheck(&mc_data_0234[2], 2);
+
+	err = ar0234_write(client, mc_data_0234, TX_LEN_PKT);
+	if (err != 0)
+	{
+		dev_err(&client->dev, "MCU CMD ID version Error-  %d\n", err);
+		ret = -EIO;
+		goto exit;
+	}
+
+	mc_data_0234[0] = CMD_SIGNATURE;
+	mc_data_0234[1] = CMD_ID_VERSION;
+	err = ar0234_write(client, mc_data_0234, 2);
+	if (err != 0) {
+		dev_err(&client->dev," %s(%d) MCU CMD ID Write PKT fw Version Error - %d \n", __func__,
+				__LINE__, err);
+		ret = -EIO;
+		goto exit;
+	}
+
+	err = ar0234_read(client, mc_ret_data_0234, RX_LEN_PKT);
+	if (err != 0) {
+		dev_err(&client->dev," %s(%d) MCU CMD ID Read PKT fw Version Error - %d \n", __func__,
+				__LINE__, err);
+		ret = -EIO;
+		goto exit;
+	}
+
+	/* Verify CRC */
+	orig_crc = mc_ret_data_0234[4];
+	calc_crc = errorcheck(&mc_ret_data_0234[2], 2);
+	if (orig_crc != calc_crc) {
+		dev_err(&client->dev," %s(%d) MCU CMD ID fw Version Error CRC 0x%02x != 0x%02x \n",
+				__func__, __LINE__, orig_crc, calc_crc);
+		ret = -EINVAL;
+		goto exit;
+	}
+
+	errcode = mc_ret_data_0234[5];
+	if (errcode != ERRCODE_SUCCESS) {
+		dev_err(&client->dev," %s(%d) MCU CMD ID fw Errcode - 0x%02x \n", __func__,
+				__LINE__, errcode);
+		ret = -EIO;
+		goto exit;
+	}
+
+	/* Read the actual version from MCU*/
+	payload_len =
+	    ((mc_ret_data_0234[2] << 8) | mc_ret_data_0234[3]) + HEADER_FOOTER_SIZE;
+	memset(mc_ret_data_0234, 0x00, payload_len);
+	err = ar0234_read(client, mc_ret_data_0234, payload_len);
+	if (err != 0) {
+		dev_err(&client->dev," %s(%d) MCU fw CMD ID Read Version Error - %d \n", __func__,
+				__LINE__, err);
+		ret = -EIO;
+		goto exit;
+	}
+
+	/* Verify CRC */
+	orig_crc = mc_ret_data_0234[payload_len - 2];
+	calc_crc = errorcheck(&mc_ret_data_0234[2], 32);
+	if (orig_crc != calc_crc) {
+		dev_err(&client->dev," %s(%d) MCU fw  CMD ID Version CRC ERROR 0x%02x != 0x%02x \n",
+				__func__, __LINE__, orig_crc, calc_crc);
+		ret = -EINVAL;
+		goto exit;
+	}
+
+	/* Verify Errcode */
+	errcode = mc_ret_data_0234[payload_len - 1];
+	if (errcode != ERRCODE_SUCCESS) {
+		dev_err(&client->dev," %s(%d) MCU fw CMD ID Read Payload Error - 0x%02x \n", __func__,
+				__LINE__, errcode);
+		ret = -EIO;
+		goto exit;
+	}
+
+	for (loop = 0 ; loop < VERSION_SIZE ; loop++ )
+		*(fw_version_0234+loop) = mc_ret_data_0234[2+loop];
+
+	ret = ERRCODE_SUCCESS;
+exit:
+	/* unlock semaphore */
+	mutex_unlock(&mcu_i2c_mutex_0234);
+
+	return ret;
+}
