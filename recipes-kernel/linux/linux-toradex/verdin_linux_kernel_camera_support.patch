+static int mcu_set_ctrl(struct i2c_client *client, uint32_t arg_ctrl_id,
+			uint8_t ctrl_type, int32_t curr_val)
+{
+	uint32_t payload_len = 0;
+
+	uint16_t cmd_status = 0, index = 0xFFFF;
+	uint8_t retcode = 0, cmd_id = 0;
+	int loop = 0, ret = 0, err = 0;
+	uint32_t ctrl_id = 0;
+
+	/* lock semaphore */
+	mutex_lock(&mcu_i2c_mutex_0234);
+
+	ctrl_id = arg_ctrl_id;
+
+	/* call ISP Ctrl config command */
+
+	for (loop = 0; loop < num_ctrls; loop++) {
+		if (ctrldb[loop] == ctrl_id) {
+			index = mcu_ctrl_info[loop].mcu_ctrl_index;
+			break;
+		}
+	}
+
+	if (index == 0xFFFF) {
+		ret = -EINVAL;
+		goto exit;
+	}
+
+	/* First Txn Payload length = 0 */
+	payload_len = 11;
+
+	mc_data_0234[0] = CMD_SIGNATURE;
+	mc_data_0234[1] = CMD_ID_SET_CTRL;
+	mc_data_0234[2] = payload_len >> 8;
+	mc_data_0234[3] = payload_len & 0xFF;
+	mc_data_0234[4] = errorcheck(&mc_data_0234[2], 2);
+
+	ar0234_write(client, mc_data_0234, TX_LEN_PKT);
+
+	/* Second Txn */
+	mc_data_0234[0] = CMD_SIGNATURE;
+	mc_data_0234[1] = CMD_ID_SET_CTRL;
+
+	/* Index */
+	mc_data_0234[2] = index >> 8;
+	mc_data_0234[3] = index & 0xFF;
+
+	/* Control ID */
+	mc_data_0234[4] = ctrl_id >> 24;
+	mc_data_0234[5] = ctrl_id >> 16;
+	mc_data_0234[6] = ctrl_id >> 8;
+	mc_data_0234[7] = ctrl_id & 0xFF;
+
+	/* Ctrl Type */
+	mc_data_0234[8] = ctrl_type;
+
+	/* Ctrl Value */
+	mc_data_0234[9] = curr_val >> 24;
+	mc_data_0234[10] = curr_val >> 16;
+	mc_data_0234[11] = curr_val >> 8;
+	mc_data_0234[12] = curr_val & 0xFF;
+
+	/* CRC */
+	mc_data_0234[13] = errorcheck(&mc_data_0234[2], 11);
+
+	err = ar0234_write(client, mc_data_0234, 14);
+	if (err != 0) {
+		dev_err(&client->dev," %s(%d) Error - %d \n", __func__,
+		       __LINE__, err);
+		ret = -EIO;
+		goto exit;
+	}
+
+	while (1) {
+		cmd_id = CMD_ID_SET_CTRL;
+		if (mcu_get_cmd_status(
+			client, &cmd_id, &cmd_status, &retcode) < 0) {
+			dev_err(&client->dev," %s(%d) Error \n",
+			       __func__, __LINE__);
+			ret = -EINVAL;
+			goto exit;
+		}
+
+		if ((cmd_status == MCU_CMD_STATUS_SUCCESS) &&
+		    (retcode == ERRCODE_SUCCESS)) {
+			ret = 0;
+			goto exit;
+		}
+
+		if ((retcode != ERRCODE_BUSY) &&
+		    ((cmd_status != MCU_CMD_STATUS_PENDING))) {
+			dev_err(&client->dev,
+				"(%s) %d ISP Error STATUS = 0x%04x RET = 0x%02x\n",
+			     __func__, __LINE__, cmd_status, retcode);
+			ret = -EIO;
+			goto exit;
+		}
+	}
+
+ exit:
+	/* unlock semaphore */
+	mutex_unlock(&mcu_i2c_mutex_0234);
+
+	return ret;
+}
