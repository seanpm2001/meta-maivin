+static int ar0234_enum_frameintervals(struct v4l2_subdev *sd,struct v4l2_subdev_pad_config *cfg,struct v4l2_subdev_frame_interval_enum *fival)
+{
+	struct i2c_client *client = v4l2_get_subdevdata(sd);
+	int j;
+
+	if (fival->width == 0 || fival->height == 0)
+	{
+		dev_err(&client->dev, "Please assign width and height.\n");
+		return -EINVAL;
+	}
+
+	if (fival->code != ar0234_data.fmt.code)
+	{
+		return -EINVAL;
+	}
+
+	for (j = 0; j < ar0234_data.num_frm_fmts; j++) {
+		if (
+			fival->width == ar0234_data.mcu_cam_frmfmt[j].size.width &&
+			fival->height == ar0234_data.mcu_cam_frmfmt[j].size.height
+		) {
+			if (fival->index >= ar0234_data.mcu_cam_frmfmt[j].num_framerates)
+			{
+				return -EINVAL;
+			}
+
+			fival->interval.numerator = 1;
+			fival->interval.denominator = ar0234_data.mcu_cam_frmfmt[j].framerates[fival->index];
+
+			return 0;
+		}
+	}
+	return -EINVAL;
+
+}
+
+static int ar0234_enum_framesizes(struct v4l2_subdev *sd,struct v4l2_subdev_pad_config *cfg,struct v4l2_subdev_frame_size_enum *fse)
+{
+	if (fse->index >= ar0234_data.num_frm_fmts)
+	{
+		return -EINVAL;
+	}
+
+	if (fse->code != ar0234_data.fmt.code)
+	{
+		return -EINVAL;
+	}
+
+	fse->max_width = ar0234_data.mcu_cam_frmfmt[fse->index].size.width;
+	fse->min_width = fse->max_width;
+
+	fse->max_height = ar0234_data.mcu_cam_frmfmt[fse->index].size.height;
+	fse->min_height = fse->max_height;
+
+	return 0;
+}
