+static int ar0234_get_fmt(struct v4l2_subdev *sd,struct v4l2_subdev_pad_config *cfg,struct v4l2_subdev_format *format)
+{
+	int ret = 0;
+
+	if (format->pad)
+		return -EINVAL;
+
+	format->format.code = ar0234_data.fmt.code;
+	format->format.colorspace = ar0234_data.fmt.colorspace;
+	format->format.field = V4L2_FIELD_NONE;
+
+	format->format.width	= ar0234_data.pix.width;
+	format->format.height	= ar0234_data.pix.height;
+
+	return ret;
+}
+
+static int ar0234_set_fmt(struct v4l2_subdev *sd, struct v4l2_subdev_pad_config *cfg,
+			  struct v4l2_subdev_format *format)
+{
+	int ret = 0, i;
+	struct i2c_client *client = v4l2_get_subdevdata(sd);
+	int flag = 0, err = 0;
+
+	format->format.code = ar0234_data.fmt.code;
+	format->format.colorspace = ar0234_data.fmt.colorspace;
+	format->format.field = V4L2_FIELD_NONE;
+
+	for (i = 0; i < ar0234_data.num_frm_fmts ; i++) {
+		if (
+			ar0234_data.mcu_cam_frmfmt[i].size.width == format->format.width &&
+			ar0234_data.mcu_cam_frmfmt[i].size.height == format->format.height
+		) {
+			flag = 1;
+			break;
+		}
+	}
+
+	if(flag == 0) {
+		format->format.width	= ar0234_data.pix.width;
+		format->format.height	= ar0234_data.pix.height;
+	}
+
+	if (format->which == V4L2_SUBDEV_FORMAT_TRY)
+	{
+		return 0;
+	}
+
+	/* call stream config with width, height, frame rate */
+	err =
+		mcu_stream_config(client, ar0234_data.pix.pixelformat, ar0234_data.mcu_cam_frmfmt[i].mode,
+				ar0234_data.frate_index);
+	if (err < 0) {
+		dev_err(&client->dev, "%s: Failed stream_config \n", __func__);
+		return err;
+	}
+
+	ar0234_data.pix.width = format->format.width;
+	ar0234_data.pix.height = format->format.height;
+	ar0234_data.streamcap.capturemode = ar0234_data.mcu_cam_frmfmt[i].mode;
+
+	mdelay(10);
+
+	return ret;
+}
