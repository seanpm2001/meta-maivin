+/**
+ * mcu_verify_fw_version:
+ *
+ * Verify the firmware version obtained from the MCU and that found in the
+ * firmware text file present in our driver.
+ *
+ * The return value after verification is as follows:
+ *
+ *   - If the version number matches a success value (0) is returned.
+ *
+ *   - In case the version number mismatches, a negative value indicating error
+ *     is returned.
+ *
+ *   - In case the  force update bit is set in firmware version in the text
+ *     file, a positive value is returned.
+ */
+static int mcu_verify_fw_version(const unsigned char *const fw_version_0234)
+{
+	int loop, i = 0, ret;
+	char txt_fw_version[32] = {0};
+	unsigned long txt_fw_pos = ARRAY_SIZE(g_mcu_fw_buf_ar0234)-VERSION_FILE_OFFSET;
+
+	/* Get Text Firmware version*/
+	for(loop = txt_fw_pos; loop < (txt_fw_pos+64); loop=loop+2) {
+		*(txt_fw_version+i) = (mcu_bload_ascii2hex(g_mcu_fw_buf_ar0234[loop]) << 4 |
+				mcu_bload_ascii2hex(g_mcu_fw_buf_ar0234[loop+1]));
+		i++;
+	}
+
+	/* Check for forced/always update field in the text firmware version*/
+	if(txt_fw_version[17] == '1') {
+
+#ifdef AR0234_DEBUG
+		pr_info("Forced Update Enabled - Firmware Version - (%.32s) \n",
+			fw_version_0234);
+#endif
+
+		ret = 2;
+	}
+	else {
+		for(i = 0; i < VERSION_SIZE; i++) {
+			if(txt_fw_version[i] != fw_version_0234[i]) {
+
+				pr_info("Previous Firmware Version - (%.32s)\n", fw_version_0234);
+
+				ret = -1;
+				break;
+			}
+		}
+
+		if (i == VERSION_SIZE)
+			ret = ERRCODE_SUCCESS;
+	}
+
+	return ret;
+}
+
+static int mcu_bload_get_version(struct i2c_client *client)
+{
+	int ret = 0;
+
+	/*----------------------------- GET VERSION -------------------- */
+
+	/*   Write Get Version CMD */
+	g_bload_buf_0234[0] = BL_GET_VERSION;
+	g_bload_buf_0234[1] = ~(BL_GET_VERSION);
+
+	ret = ar0234_write(client, g_bload_buf_0234, 2);
+	if (ret < 0) {
+		dev_err(&client->dev,"Write Failed \n");
+		return -1;
+	}
+
+	/*   Wait for ACK or NACK */
+	ret = ar0234_read(client, g_bload_buf_0234, 1);
+	if (ret < 0) {
+		dev_err(&client->dev,"Read Failed \n");
+		return -1;
+	}
+
+	if (g_bload_buf_0234[0] != 'y') {
+		/*   NACK Received */
+		dev_err(&client->dev," NACK Received... exiting.. \n");
+		return -1;
+	}
+
+	ret = ar0234_read(client, g_bload_buf_0234, 1);
+	if (ret < 0) {
+		dev_err(&client->dev,"Read Failed \n");
+		return -1;
+	}
+
+	ret = ar0234_read(client, g_bload_buf_0234, 1);
+	if (ret < 0) {
+		dev_err(&client->dev,"Read Failed\n");
+		return -1;
+	}
+
+	/* ---------------- GET VERSION END ------------------- */
+
+	return 0;
+}
+
+static int mcu_bload_erase_flash(struct i2c_client *client)
+{
+	unsigned short int pagenum = 0x0000;
+	int ret = 0, i = 0, checksum = 0;
+
+	/* --------------- ERASE FLASH --------------------- */
+
+	for (i = 0; i < NUM_ERASE_CYCLES; i++) {
+
+		checksum = 0x00;
+		/*   Write Erase Pages CMD */
+		g_bload_buf_0234[0] = BL_ERASE_MEM_NS;
+		g_bload_buf_0234[1] = ~(BL_ERASE_MEM_NS);
+
+		ret = ar0234_write(client, g_bload_buf_0234, 2);
+		if (ret < 0) {
+			dev_err(&client->dev,"Write Failed \n");
+			return -1;
+		}
+
+		/*   Wait for ACK or NACK */
+		ret = ar0234_read(client, g_bload_buf_0234, 1);
+		if (ret < 0) {
+			dev_err(&client->dev,"Read Failed \n");
+			return -1;
+		}
+
+		if (g_bload_buf_0234[0] != RESP_ACK) {
+			/*   NACK Received */
+			dev_err(&client->dev," NACK Received... exiting.. \n");
+			return -1;
+		}
+
+		g_bload_buf_0234[0] = (MAX_PAGES - 1) >> 8;
+		g_bload_buf_0234[1] = (MAX_PAGES - 1) & 0xFF;
+		g_bload_buf_0234[2] = g_bload_buf_0234[0] ^ g_bload_buf_0234[1];
+
+		ret = ar0234_write(client, g_bload_buf_0234, 3);
+		if (ret < 0) {
+			dev_err(&client->dev,"Write Failed \n");
+			return -1;
+		}
+
+		/*   Wait for ACK or NACK */
+		ret = ar0234_read(client, g_bload_buf_0234, 1);
+		if (ret < 0) {
+			dev_err(&client->dev,"Read Failed \n");
+			return -1;
+		}
+
+		if (g_bload_buf_0234[0] != RESP_ACK) {
+			/*   NACK Received */
+			dev_err(&client->dev," NACK Received... exiting.. \n");
+			return -1;
+		}
+
+		for (pagenum = 0; pagenum < MAX_PAGES; pagenum++) {
+			g_bload_buf_0234[(2 * pagenum)] =
+			    (pagenum + (i * MAX_PAGES)) >> 8;
+			g_bload_buf_0234[(2 * pagenum) + 1] =
+			    (pagenum + (i * MAX_PAGES)) & 0xFF;
+			checksum =
+			    checksum ^ g_bload_buf_0234[(2 * pagenum)] ^
+			    g_bload_buf_0234[(2 * pagenum) + 1];
+		}
+		g_bload_buf_0234[2 * MAX_PAGES] = checksum;
+
+		ret = ar0234_write(client, g_bload_buf_0234, (2 * MAX_PAGES) + 1);
+		if (ret < 0) {
+			dev_err(&client->dev,"Write Failed \n");
+			return -1;
+		}
+
+ poll_busy:
+		/*   Wait for ACK or NACK */
+		ret = ar0234_read(client, g_bload_buf_0234, 1);
+		if (ret < 0) {
+			dev_err(&client->dev,"Read Failed \n");
+			return -1;
+		}
+
+		if (g_bload_buf_0234[0] == RESP_BUSY)
+			goto poll_busy;
+
+		if (g_bload_buf_0234[0] != RESP_ACK) {
+			/*   NACK Received */
+			dev_err(&client->dev," NACK Received... exiting.. \n");
+			return -1;
+		}
+
+#ifdef AR0234_DEBUG
+		pr_info(" ERASE Sector %d success !! \n", i + 1);
+#endif
+	}
+
+	/* ------------ ERASE FLASH END ----------------------- */
+
+	return 0;
+}
+
+static unsigned char mcu_bload_inv_checksum(unsigned char *buf, int len)
+{
+	unsigned int checksum = 0x00;
+	int i = 0;
+
+	if (!buf || !(buf + len))
+		return 0;
+
+	for (i = 0; i < len; i++) {
+		checksum = (checksum + buf[i]);
+	}
+
+	checksum &= (0xFF);
+	return (~(checksum) + 1);
+}
+
+static int mcu_bload_parse_send_cmd(struct i2c_client *client,
+			     unsigned char *bytearray, int rec_len)
+{
+	IHEX_RECORD *ihex_rec = NULL;
+	unsigned char checksum = 0, calc_checksum = 0;
+	int i = 0, ret = 0;
+
+	if (!bytearray)
+		return -1;
+
+	ihex_rec = (IHEX_RECORD *) bytearray;
+	ihex_rec->addr = htons(ihex_rec->addr);
+
+	checksum = bytearray[rec_len - 1];
+
+	calc_checksum = mcu_bload_inv_checksum(bytearray, rec_len - 1);
+	if (checksum != calc_checksum) {
+		dev_err(&client->dev," Invalid Checksum 0x%02x != 0x%02x !! \n",
+		       checksum, calc_checksum);
+		return -1;
+	}
+
+	if ((ihex_rec->rectype == REC_TYPE_ELA)
+	    && (ihex_rec->addr == 0x0000)
+	    && (ihex_rec->datasize = 0x02)) {
+		/*   Upper 32-bit configuration */
+		g_bload_flashaddr_0234 = (ihex_rec->recdata[0] <<
+				     24) | (ihex_rec->recdata[1]
+					    << 16);
+#ifdef AR0234_DEBUG
+		pr_info("Updated Flash Addr = 0x%08x \n",
+			     g_bload_flashaddr_0234);
+#endif
+
+	} else if (ihex_rec->rectype == REC_TYPE_DATA) {
+		/*   Flash Data into Flashaddr */
+
+		g_bload_flashaddr_0234 =
+		    (g_bload_flashaddr_0234 & 0xFFFF0000) | (ihex_rec->addr);
+		g_bload_crc16_ar0234 ^=
+		    mcu_bload_calc_crc16(ihex_rec->recdata, ihex_rec->datasize);
+
+		/*   Write Erase Pages CMD */
+		g_bload_buf_0234[0] = BL_WRITE_MEM_NS;
+		g_bload_buf_0234[1] = ~(BL_WRITE_MEM_NS);
+
+		ret = ar0234_write(client, g_bload_buf_0234, 2);
+		if (ret < 0) {
+			dev_err(&client->dev,"Write Failed \n");
+			return -1;
+		}
+
+		/*   Wait for ACK or NACK */
+		ret = ar0234_read(client, g_bload_buf_0234, 1);
+		if (ret < 0) {
+			dev_err(&client->dev,"Read Failed \n");
+			return -1;
+		}
+
+		if (g_bload_buf_0234[0] != RESP_ACK) {
+			/*   NACK Received */
+			dev_err(&client->dev," NACK Received... exiting.. \n");
+			return -1;
+		}
+
+		g_bload_buf_0234[0] = (g_bload_flashaddr_0234 & 0xFF000000) >> 24;
+		g_bload_buf_0234[1] = (g_bload_flashaddr_0234 & 0x00FF0000) >> 16;
+		g_bload_buf_0234[2] = (g_bload_flashaddr_0234 & 0x0000FF00) >> 8;
+		g_bload_buf_0234[3] = (g_bload_flashaddr_0234 & 0x000000FF);
+		g_bload_buf_0234[4] =
+		    g_bload_buf_0234[0] ^ g_bload_buf_0234[1] ^ g_bload_buf_0234[2] ^
+		    g_bload_buf_0234[3];
+
+		ret = ar0234_write(client, g_bload_buf_0234, 5);
+		if (ret < 0) {
+			dev_err(&client->dev,"Write Failed \n");
+			return -1;
+		}
+
+		/*   Wait for ACK or NACK */
+		ret = ar0234_read(client, g_bload_buf_0234, 1);
+		if (ret < 0) {
+			dev_err(&client->dev,"Read Failed \n");
+			return -1;
+		}
+
+		if (g_bload_buf_0234[0] != RESP_ACK) {
+			/*   NACK Received */
+			dev_err(&client->dev," NACK Received... exiting.. \n");
+			return -1;
+		}
+
+		g_bload_buf_0234[0] = ihex_rec->datasize - 1;
+		checksum = g_bload_buf_0234[0];
+		for (i = 0; i < ihex_rec->datasize; i++) {
+			g_bload_buf_0234[i + 1] = ihex_rec->recdata[i];
+			checksum ^= g_bload_buf_0234[i + 1];
+		}
+
+		g_bload_buf_0234[i + 1] = checksum;
+
+		ret = ar0234_write(client, g_bload_buf_0234, i + 2);
+		if (ret < 0) {
+			dev_err(&client->dev,"Write Failed \n");
+			return -1;
+		}
+
+ poll_busy:
+		/*   Wait for ACK or NACK */
+		ret = ar0234_read(client, g_bload_buf_0234, 1);
+		if (ret < 0) {
+			dev_err(&client->dev,"Read Failed \n");
+			return -1;
+		}
+
+		if (g_bload_buf_0234[0] == RESP_BUSY)
+			goto poll_busy;
+
+		if (g_bload_buf_0234[0] != RESP_ACK) {
+			/*   NACK Received */
+			dev_err(&client->dev," NACK Received... exiting.. \n");
+			return -1;
+		}
+
+	} else if (ihex_rec->rectype == REC_TYPE_SLA) {
+		/*   Update Instruction pointer to this address */
+
+	} else if (ihex_rec->rectype == REC_TYPE_EOF) {
+		/*   End of File - Issue I2C Go Command */
+		return 0;
+	} else {
+
+		/*   Unhandled Type */
+		dev_err(&client->dev,"Unhandled Command Type \n");
+		return -1;
+	}
+
+	return 0;
+}
